-- Selecione um funcionário e atualize o Cargo tendo 12% de aumento de salário

UPDATE MC_FUNCIONARIO 
SET VL_SALARIO = VL_SALARIO * 1.12, DS_CARGO = 'GERENTE DE RH' 
WHERE CD_FUNCIONARIO = 21;

-- Atualize a descrição de uma categoria de produto

UPDATE MC_CATEGORIA_PROD
SET ds_categoria = 'Jogos'
WHERE cd_categoria = 8;

-- Atualize o nome de um departamento, utilizando como filtro o nome do departamento antes de ser atualizado

UPDATE MC_DEPTO
SET NM_DEPTO = 'GESTÃO DE PESSOAS'
WHERE NM_DEPTO = 'RECURSOS HUMANOS (RH)';

-- Atualize a data de nascimento de um cliente pessoa física, definindo uma nova data

UPDATE mc_cli_fisica
SET dt_nascimento = TO_DATE('18/05/2002', 'dd/mm/yyyy')
WHERE nr_cliente = 9;

-- Atualize a descrição de uma categoria de vídeo

UPDATE mc_categoria_prod
SET ds_categoria = 'Comercial SEM personalidade'
WHERE cd_categoria = 16;

-- Desative um funcionário colocando o status como I(nativo) e também a data de desligamento como sendo a data de hoje (sysdate).

UPDATE MC_FUNCIONARIO
SET ST_FUNC = 'I', DT_DESLIGAMENTO = SYSDATE
WHERE CD_FUNCIONARIO = 8;

-- Cadastre um atendimento SAC, atualize a descrição detalhada de retorno do SAC feito pelo funcionário. Insira um conteúdo significativo, atualize também a data e hora de atendimento, acrescendo o número total de horas do atendimento SAC.

-- Cadastrando SAC
INSERT INTO MC_SGV_SAC (nr_sac, nr_cliente, cd_produto, cd_funcionario, tp_sac, ds_detalhada_sac, dt_abertura_sac, hr_abertura_sac) 
     values (SQ_MC_SGV_SAC.NEXTVAL, 1, 1, 1,'D', 'O jarro quebra com facilidade? Se sim, possui garantia?', TO_DATE('04/04/2023', 'dd/mm/yyyy'), 10);
-- Editando retorno do SAC
UPDATE MC_SGV_SAC
SET ds_detalhada_retorno_sac = 'Não, o jarro é bem resistente, mas oferecemos garantia adicional', 
    dt_atendimento = TO_DATE('04/04/2023', 'dd/mm/yyyy'),
    hr_atendimento_sac = 13,
    nr_tempo_total_sac = 13 - hr_abertura_sac
WHERE nr_sac = 1;

-- Selecione o endereço de um cliente e coloque o status como I(nativo), preencha a data de término como sendo a data de ontem

UPDATE MC_END_CLI
SET st_end = 'I', dt_termino = SYSDATE - 1
WHERE nr_cliente = 1 AND cd_logradouro_cli = 1;

-- Selecione um endereço de funcionário e coloque o status como I(nativo), preencha a data de término como sendo a data de ontem

UPDATE MC_END_FUNC
SET st_end = 'I', dt_termino = SYSDATE - 1
WHERE cd_funcionario = 1 AND cd_logradouro = 1;
